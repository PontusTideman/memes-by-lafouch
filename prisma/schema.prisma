generator client {
  provider = "prisma-client-js"
}

datasource db {
  // provider  = "postgresql"
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Meme {
  id    String @id @default(cuid())
  title String
  videoUrl String
  downloadCount Int @default(0)
  videoKey String  @unique
  twitterUrl String?
  createdAt     DateTime       @default(now())
  updatedAt     DateTime       @updatedAt
}
